<VBox alignment="CENTER" prefHeight="610.0" prefWidth="613.0" spacing="20.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="clientapp.listaobecnosci.HomeController">
