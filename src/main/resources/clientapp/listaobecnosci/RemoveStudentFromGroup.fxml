                <Label layoutX="314.0" layoutY="241.0" styleClass="formLabel" text="Indeks Studenta" />
                <Button layoutX="125.0" layoutY="368.0" mnemonicParsing="false" onAction="#onDeleteClick" prefHeight="25.0" prefWidth="361.0" text="Usuń" />
